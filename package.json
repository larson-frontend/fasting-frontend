{
  "name": "fasting-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --reporter=verbose --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "lint:fix": "echo 'Lint fixing not configured yet'",
    "validate": "npm run type-check && npm run test:run",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "echo 'Prepare hook for CI/CD'",
    "postinstall": "echo 'Dependencies installed successfully'"
  },
  "dependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.1.0"
  }
}
